{
  "name": "snowflake",
  "displayName": "Snowflake Theme",
  "author": "Filip Brebera <breberaf@gmail.com>",
  "license": "MIT",
  "publisher": "breberaf",
  "repository": {
    "type": "git",
    "url": "https://github.com/breberafilip/snowflake-theme-vscode"
  },
  "galleryBanner": {
		"color": "#1b2029",
		"theme": "dark"
	},
  "description": "Modern color theme in the colors of winter.",
  "scripts": {
    "build": "node build/light && node build/dark"
  },
  "icon": "assets/icon.png",
  "version": "1.0.9",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "dark",
    "theme",
    "snowflake",
    "winter",
    "cold"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Snowflake Light",
        "uiTheme": "vs",
        "path": "./themes/light/snowflake.json"
      },
      {
        "label": "Snowflake Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/snowflake.json"
      },
      {
        "label": "Snowflake Contrast",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/snowflake-contrast.json"
      }
    ]
  },
  "devDependencies": {
    "mustache": "^4.0.1"
  }
}
